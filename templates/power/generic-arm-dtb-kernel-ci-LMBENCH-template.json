{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "metadata": {
                "image.type": "{image_type}",
                "image.url": "{image_url}",
                "kernel.tree": "{tree}",
                "kernel.version": "{kernel_version}",
                "device.tree": "{device_tree}",
                "kernel.endian": "{endian}",
                "kernel.defconfig": "{defconfig}",
                "platform.fastboot": "{fastboot}",
                "test.plan": "{test_plan}",
                "test.desc": "LMBENCH Hackbench power test"
            },
            "parameters": {
                "dtb": "{dtb_url}",
                "kernel": "{kernel_url}",
                "overlays": ["{modules_url}"],
                "nfsrootfs": "http://releases.linaro.org/15.06/openembedded/images/lng-armv7a/linaro-image-lng-genericarmv7a-20150618-335.rootfs.tar.gz"
            }
        },
        {
            "command": "lava_command_run",
            "parameters": {
                "commands": [
			"for i in {1..1000}; do timeout 10s hackbench | tee -a hackbench_res.txt; done"
                ],
                "timeout": 1810
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "LAVA_SERVER",
                "stream": "BUNDLE_STREAM"
            }
        }
    ],
    "device_type": "{device_type}",
    "job_name": "{job_name}",
    "logging_level": "DEBUG",
    "priority": "{priority}",
    "timeout": 1811
}
